{
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "tabWidth": 2,
    "printWidth": 100,
    "endOfLine": "lf",
    "arrowParens": "always",
    "bracketSpacing": true,
    "jsxSingleQuote": false,
    "jsxBracketSameLine": false,
    "plugins": ["prettier-plugin-tailwindcss","@trivago/prettier-plugin-sort-imports"],
    "importOrder": [
    "^react", 
    "<THIRD_PARTY_MODULES>", 
    "^@/(.*)$", 
    "^[./]"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true
  }
  